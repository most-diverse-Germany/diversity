{
  "name": "diversity-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "chart.js": "^3.2.1",
    "concurrently": "^6.1.0",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.12.8",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react-chartjs-2": "^3.0.3",
    "react-spring": "^9.1.2",
    "tailwindcss": "^2.1.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
